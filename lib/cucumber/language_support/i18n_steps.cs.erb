namespace Cuke4Nuke.Framework.Languages.<%= language %>
{
    <% unless given_keyword.nil? %>
    public class <%= given_keyword %>Attribute : GivenAttribute
    {
        public <%= given_keyword %>Attribute(string pattern) : base(pattern) { }
    }
    <% end %>

    <% unless when_keyword.nil? %>
    public class <%= when_keyword %>Attribute : WhenAttribute
    {
        public <%= when_keyword %>Attribute(string pattern): base(pattern) { }
    }
    <%  end %>

    <% unless then_keyword.nil? %>
    public class <%= then_keyword %>Attribute : ThenAttribute
    {
        public <%= then_keyword %>Attribute(string pattern): base(pattern) { }
    }
    <%  end %>
}
